{
  "name": "30-seconds-web",
  "private": true,
  "version": "1.0.0",
  "description": "Website for 30-seconds projects.",
  "main": "index.js",
  "author": "30-seconds",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@chalarangelo/combine-class-names": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-gatsby": "^0.4.8",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "cypress": "^4.12.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "esm": "^3.2.25",
    "front-matter": "^3.1.0",
    "fs-extra": "^9.0.0",
    "gatsby": "^2.21.12",
    "gatsby-plugin-catch-links": "^2.1.27",
    "gatsby-plugin-manifest": "^2.2.45",
    "gatsby-plugin-netlify": "^2.1.34",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-plugin-sitemap": "^2.2.28",
    "gatsby-source-filesystem": "^2.1.51",
    "glob": "^7.1.6",
    "hast-util-to-html": "^7.1.1",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "kleur": "^3.0.3",
    "lighthouse": "^6.0.0",
    "mdast-util-to-hast": "^9.1.0",
    "node-sass": "^4.14.1",
    "npm-watch": "^0.6.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "remark": "^12.0.0",
    "sass-loader": "^8.0.2",
    "sharp": "^0.25.4",
    "start-server-and-test": "^1.10.9",
    "style-loader": "^1.1.3",
    "unist-util-visit": "^2.0.2",
    "webfonts-generator": "^0.4.0"
  },
  "keywords": [
    "gatsby",
    "30-seconds",
    "snippets"
  ],
  "license": "MIT",
  "scripts": {
    "extract-snippets": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js -e",
    "watch-snippets": "npm-watch extract-snippets",
    "prepare-assets": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js -e -n=DEVELOPMENT",
    "watch-assets": "npm-watch prepare-assets",
    "serve-snippets": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js -n=DEVELOPMENT -s",
    "yild": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js",
    "prebuild": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js -u -a -e -i -b -n=PRODUCTION -c",
    "predevelop": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js -n=DEVELOPMENT -c",
    "pretest": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/yild.js -n=DEVELOPMENT -c",
    "build": "NODE_PATH=src npx --node-arg '-r esm' gatsby build",
    "develop": "NODE_PATH=src npx --node-arg '-r esm' gatsby develop -p 8000",
    "debug": "NODE_PATH=src npx --node-arg '-r esm --nolazy --inspect-brk' node_modules/.bin/gatsby develop",
    "serve": "NODE_PATH=src npx --node-arg '-r esm' gatsby serve",
    "lint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "cy:open": "NODE_PATH=src npx --node-arg '-r esm' cypress open --config-file src/config/cypress.json",
    "cy:run": "NODE_PATH=src npx --node-arg '-r esm' cypress run --config-file src/config/cypress.json --env CYPRESS_NO_AUDIT=true",
    "test": "cross-env NODE_PATH=src NODE_ENV=test jest --config src/config/jest.json --coverage",
    "test:watch": "cross-env NODE_PATH=src NODE_ENV=test jest --config src/config/jest.json --watch",
    "test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:open",
    "test:e2e-ci": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:run"
  },
  "watch": {
    "extract-snippets": {
      "patterns": [
        "content/sources"
      ],
      "extensions": "md",
      "delay": "5000",
      "ignore": "README",
      "quiet": true,
      "runOnChangeOnly": true
    },
    "prepare-assets": {
      "patterns": [
        "content/sources"
      ],
      "extensions": "jpeg,jpg,png,webp,tif,tiff",
      "delay": "5000",
      "quiet": true,
      "runOnChangeOnly": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/30-seconds/30-seconds-web"
  },
  "bugs": {
    "url": "https://github.com/30-seconds/30-seconds-web/issues"
  },
  "dependencies": {},
  "browserslist": [
    "> 0.5% and last 4 versions and not dead and not ie>0 and not op_mini all and not and_uc>0 and not edge<79"
  ]
}
