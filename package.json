{
  "name": "30-seconds-web",
  "private": true,
  "version": "1.0.0",
  "description": "Website for 30-seconds projects.",
  "main": "index.js",
  "author": "30-seconds",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@chalarangelo/combine-class-names": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-gatsby": "^0.2.33",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "cypress": "^3.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "esm": "^3.2.25",
    "front-matter": "^3.1.0",
    "fs-extra": "^8.1.0",
    "gatsby": "^2.21.12",
    "gatsby-plugin-catch-links": "^2.1.27",
    "gatsby-plugin-manifest": "^2.2.45",
    "gatsby-plugin-netlify": "^2.1.34",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-page-creator": "^2.1.43",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-resolve-src": "^2.0.0",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-plugin-sharp": "^2.4.8",
    "gatsby-plugin-sitemap": "^2.2.28",
    "gatsby-remark-copy-linked-files": "^2.1.39",
    "gatsby-remark-images": "^3.1.47",
    "gatsby-remark-prismjs": "^3.3.33",
    "gatsby-source-filesystem": "^2.1.51",
    "gatsby-transformer-remark": "^2.6.56",
    "gatsby-transformer-sharp": "^2.3.17",
    "glob": "^7.1.6",
    "icon-font-generator": "^2.1.10",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "kleur": "^3.0.3",
    "lighthouse": "^5.6.0",
    "node-sass": "^4.13.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.10.9",
    "style-loader": "^1.1.3"
  },
  "keywords": [
    "gatsby",
    "30-seconds",
    "snippets"
  ],
  "license": "MIT",
  "scripts": {
    "make-icons": "icon-font-generator src/icons/*.svg -o assets --html=false --json=false --types='woff2' --csspath src/styles/_icons.scss --cssfontsurl='../../assets/' --selector='.icon'",
    "make-lang-bgs": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/make_lang_bgs.js",
    "extract-snippets": "NODE_PATH=src npx --node-arg '-r esm' ./src/jobs/extract_snippets.js",
    "prebuild": "./src/jobs/prepare_env.sh PRODUCTION; ./src/jobs/fetch_content_sources.sh; ./src/jobs/prepare_cache_key.sh; npm run make-icons; npm run make-lang-bgs; npm run extract-snippets",
    "predevelop": "./src/jobs/prepare_env.sh DEVELOPMENT;./src/jobs/prepare_cache_key.sh",
    "build": "NODE_PATH=src npx --node-arg '-r esm' gatsby build",
    "develop": "NODE_PATH=src npx --node-arg '-r esm' gatsby develop",
    "debug": "NODE_PATH=src npx --node-arg '-r esm --nolazy --inspect-brk' node_modules/.bin/gatsby develop",
    "serve": "NODE_PATH=src npx --node-arg '-r esm' gatsby serve",
    "lint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "cy:open": "NODE_PATH=src npx --node-arg '-r esm' cypress open --config-file src/config/cypress.json",
    "cy:run": "NODE_PATH=src npx --node-arg '-r esm' cypress run --config-file src/config/cypress.json --env CYPRESS_NO_AUDIT=true",
    "test": "cross-env NODE_PATH=src NODE_ENV=test jest --config src/config/jest.json --coverage",
    "test:watch": "cross-env NODE_PATH=src NODE_ENV=test jest --config src/config/jest.json --watch",
    "test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:open",
    "test:e2e-ci": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/30-seconds/30-seconds-web"
  },
  "bugs": {
    "url": "https://github.com/30-seconds/30-seconds-web/issues"
  },
  "dependencies": {},
  "browserslist": [
    "> 0.5% and last 4 versions and not dead and not ie>0 and not op_mini all and not and_uc>0 and not edge<79"
  ]
}
