    return typeof p === 'string' ? ".".concat(p) : "[".concat(p, "]");
  }).join('');
  var jsValue = value === undefined ? 'undefined' : JSON.stringify(value);
  return "".concat(objectName).concat(assignmentPath, " = ").concat(jsValue, ";");
