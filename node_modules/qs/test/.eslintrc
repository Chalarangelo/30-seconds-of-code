{
    "rules": {
		"array-bracket-newline": 0,
		"array-element-newline": 0,
		"consistent-return": 2,
        "function-paren-newline": 0,
        "max-lines": 0,
        "max-lines-per-function": 0,
        "max-nested-callbacks": [2, 3],
        "max-statements": 0,
		"no-buffer-constructor": 0,
        "no-extend-native": 0,
        "no-magic-numbers": 0,
		"object-curly-newline": 0,
        "sort-keys": 0
    }
}
