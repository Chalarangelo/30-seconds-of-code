@font-face {
  font-family: "{{ fontName }}";
  src: {{{ src }}};
}

{{ baseSelector }}:before {
  font-family: {{ fontName }} !important;
  font-style: normal;
  font-weight: normal !important;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

{{# each codepoints }}
{{ ../baseSelector }}.{{ ../classPrefix }}{{ @key }}:before {
  content: "\\{{ this }}";
}
{{/ each }}
